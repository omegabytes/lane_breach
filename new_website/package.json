{
  "name": "lane-breach",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dc:fresh": "docker-compose up --build",
    "dc:refresh": "docker-compose down -v && yarn dc:fresh",
    "dc:prune": "docker system prune --all --force --volumes",
    "dev": "concurrently \"yarn dev:app\" \"yarn dev:server\"",
    "dev:app": "webpack-dev-server --config webpack/frontend.dev",
    "dev:server": "node webpack/backend.dev.js",
    "prod:build": "yarn prod:clear && webpack --config webpack/frontend.prod && webpack --config webpack/backend.prod",
    "prod:start": "yarn prod:build && yarn prod:run",
    "prod:run": "node public/dist/bundle-backend.js",
    "prod:clear": "rimraf ./public/dist/*",
    "start:pm2": "pm2-runtime -i max server/"
  },
  "author": "",
  "license": "ISC",
